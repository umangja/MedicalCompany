    <bean id="userdao" class="com.umang.dao.UserdaoImpl"></bean>
    <bean id="addressdao" class="com.umang.dao.AddressdaoImpl"></bean>
    <bean id="managesdao" class="com.umang.dao.ManagesdaoImpl"></bean>
    <bean id="locationdao" class="com.umang.dao.LocationdaoImpl"></bean>
