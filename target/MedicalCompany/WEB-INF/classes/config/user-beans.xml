    <bean id="medicinedao" class="com.umang.dao.MedicinedaoImpl"></bean>
    <bean id="diseasedao" class="com.umang.dao.DiseasedaoImpl"></bean>
    <bean id="cancuredao" class="com.umang.dao.CancuredaoImpl"></bean>
    <bean id="containsdao" class="com.umang.dao.ContainsdaoImpl"></bean>
    <bean id="billdao"     class="com.umang.dao.BilldaoImpl"></bean>
    <bean id="partnerdao" class="com.umang.dao.PartnerdaoImpl"></bean>
    <bean id="patientdao" class="com.umang.dao.PatientdaoImpl"></bean>
    <bean id="ordersdao" class="com.umang.dao.OrdersdaoImpl"></bean>
